
int afficheNB(int nb)
{
printf("%d", nb);
	return (0);
}


int		main(int argc, char **argv)
{
	int		tab[3] = {8, 9, 10};
	int*	res;

	res = ft_map(tab, 3, &afficheNB);

	printf("%d,%d,%d",res[0],res[1],res[2]);

	return (0);
}


#include <stdio.h>

int test(int a, int b)
{
	return (a - b);
}

int main()
{
	int tab[] = { 1, 2, 3, 4, 5};

	printf("%d", ft_is_sort(tab, 5, &test));
	return(0);
}
